<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="PlantFoodRawBase">
		<defName>Raweggplant</defName>
		<label>Eggplant Fruit</label>
		<description>A large, purple, egg-shaped fruit of the Eggplant Bush. Eggplant is related to the plants of tomatoes, potatoes and tobacco, which makes it repulsive when eaten raw.</description>
		<graphicData>
			<texPath>Things/Veg/eggplant</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.3</MarketValue>
			<Nutrition>0.11</Nutrition>
		</statBases>
		<tickerType>Rare</tickerType>   
		<thingCategories>
			<li>BasicPlantFoodRaw</li>
		</thingCategories>
		<ingestible>
			<foodType>VegetableOrFruit</foodType>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>8</daysToRotStart>
			</li>
		</comps>    
	</ThingDef>


	<ThingDef ParentName="PlantFoodRawBase">
		<defName>RawLettuce</defName>
		<label>Cabbage</label>
		<description>Cabbage is the edible leaves of the Cabbage Plant. It is an essential part in many salads. Enjoyable to eat even when raw.</description>
		<graphicData>
			<texPath>Things/Veg/Cabbage</texPath>
		</graphicData>
		<statBases>
			<MarketValue>6</MarketValue>
			<Nutrition>0.07</Nutrition>
		</statBases>
		<tickerType>Rare</tickerType>   
		<ingestible>
			<foodType>VegetableOrFruit</foodType>
		</ingestible>
		<thingCategories>
			<li>ExtraPlantFoodRaw</li>
		</thingCategories>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>14</daysToRotStart>
			</li>
		</comps>
		<stackLimit>25</stackLimit>
	</ThingDef>  

</Defs>