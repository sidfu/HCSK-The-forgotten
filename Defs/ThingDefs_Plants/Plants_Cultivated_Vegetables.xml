<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="PlantDef">
		<defName>Planteggplant</defName>
		<label>Eggplant Bush</label>
		<description>Eggplant Bushes produce large, purple, egg-shaped fruits called Eggplant Fruit. Eggplant Bushes grow quickly, as the plant is related to the fast growing Potato Plant.</description>
		<graphicData>
			<texPath>Things/Plant/PlantEggplant_Harvest</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<plant>
			<maxMeshCount>4</maxMeshCount>
			<sowMinSkill>8</sowMinSkill>
			<sowResearchPrerequisites>
				<li>Food_Plants_A2</li>
			</sowResearchPrerequisites>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<immatureGraphicPath>Things/Plant/PlantEggplant</immatureGraphicPath>
			<harvestedThingDef>Raweggplant</harvestedThingDef>
			<harvestYield>14</harvestYield>
			<topWindExposure>0.1</topWindExposure>
			<growDays>5.7</growDays>
			<fertilitySensitivity>0.7</fertilitySensitivity>
			<fertilityMin>0.8</fertilityMin>
			<visualSizeRange>
				<min>0.2</min>
				<max>0.9</max>
			</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>
	
		<ThingDef ParentName="PlantDef">
		<defName>PlantCabbage</defName>
		<label>Cabbage Plant</label>
		<description>Cabbage Plants produce large, edible leaves called Cabbage. Cabbage is an essential component in many salads.</description>
		<statBases>
			<Beauty>2</Beauty>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/PlantCabbage_Harvest</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<plant>
			<sowMinSkill>6</sowMinSkill>
			<sowResearchPrerequisites>
				<li>Food_Plants_A2</li>
			</sowResearchPrerequisites>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<immatureGraphicPath>Things/Plant/PlantCabbage</immatureGraphicPath>
			<harvestedThingDef>RawLettuce</harvestedThingDef>
			<harvestYield>9</harvestYield>
			<harvestMinGrowth>0.6</harvestMinGrowth>
			<topWindExposure>0.1</topWindExposure>
			<growDays>4.1</growDays>
			<fertilitySensitivity>0.8</fertilitySensitivity>
			<fertilityMin>0.9</fertilityMin>
			<visualSizeRange>
				<min>0.3</min>
				<max>1.05</max>
			</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

</Defs>